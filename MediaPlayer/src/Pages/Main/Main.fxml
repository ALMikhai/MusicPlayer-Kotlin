<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Pages.Main.MainController">
   <children>
      <BorderPane layoutX="2.0" layoutY="2.0" prefHeight="800.0" prefWidth="600.0">
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="27.0" prefWidth="600.0">
                     <children>
                        <Text id="musicName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="450.0">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Text>
                        <Text id="musicTimer" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="150.0">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Text>
                     </children>
                  </HBox>
                  <HBox prefHeight="49.0" prefWidth="600.0">
                     <children>
                        <Slider id="musicSlider" onMouseClicked="#musicSliderClick" prefHeight="14.0" prefWidth="452.0">
                           <HBox.margin>
                              <Insets top="5.0" />
                           </HBox.margin>
                        </Slider>
                        <Slider id="volumeSlider" minorTickCount="0" onMouseDragged="#volumeSliderDragged" showTickLabels="true" showTickMarks="true" value="100.0">
                           <HBox.margin>
                              <Insets top="5.0" />
                           </HBox.margin>
                        </Slider>
                     </children>
                  </HBox>
                  <VBox id="mainVBox" prefHeight="641.0" prefWidth="600.0" />
               </children>
            </VBox>
         </center>
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                        <MenuItem mnemonicParsing="false" onAction="#addNewFile" text="Open" />
                        <MenuItem mnemonicParsing="false" onAction="#addNewFolder" text="Open as" />
                        <MenuItem mnemonicParsing="false" onAction="#settingsShow" text="Settings" />
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <bottom>
            <HBox alignment="CENTER" prefHeight="60.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Button id="prevButton" mnemonicParsing="false" onAction="#setPrevMusic">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../resources/images/prev.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button id="playButton" mnemonicParsing="false" onAction="#setMusicNow">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../resources/images/play.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button id="pauseButton" mnemonicParsing="false" onAction="#pausePlayer">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../resources/images/pause.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button id="nextButton" mnemonicParsing="false" onAction="#setNextMusic">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../resources/images/next.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button id="stopButton" mnemonicParsing="false" onAction="#stopPlayer">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../resources/images/stop.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
